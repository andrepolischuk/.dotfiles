ls=ls --color $*
pwd=cd
clear=cls
..=cd ..
...=cd ../..
....=cd ../../..
git=hub $*
ga=git add $*
gb=git branch $*
gco=git checkout $*
gc=git commit $*
gd=git diff
gl=git log --oneline --all --graph --decorate $*
gp=git push $*
gpl=git pull $*
gst=git status
gt=git tag $*
pub=git commit -am "$1" && git tag $1 && git push origin master --tags && npm publish
